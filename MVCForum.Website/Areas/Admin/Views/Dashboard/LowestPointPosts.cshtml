@model MVCForum.Website.Areas.Admin.ViewModels.LowestPointPostsViewModels

@if(Model.Posts.Any())
{
 <ul>
 @foreach (var post in Model.Posts)
 {
    <li><a href="@post.Topic.NiceUrl">@post.Topic.Name By @post.User.UserName <strong>(@post.VoteCount)</strong></a></li>
 }
 </ul>
}
else
{
    <p>No data</p>
}